	select prod, month, count( X.quant), count( Y.quant), count(Z.quant)
	from Sales
	where year=1997 or month = 2
	group by prod, month : X, Y, Z
	such that  year < 1997 or  quant > avg(X.quant) , Y.year <1996 and Y.quant < avg(quant), X.year >1995 and avg(X.quant) > 50, Z.quant > avg(X.quant)
	having  prod = "Milk" and count(Y.quant) = 1